FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application         cleopatraFoam;

startFrom           latestTime;

startTime           0;

stopAt              endTime;

endTime         	30;

deltaT          	0.05e-2;

writeControl    	adjustableRunTime;

//writeInterval   	0.05e-2;
writeInterval   	1;

purgeWrite      0;

writeFormat     binary;

writePrecision  6;

writeCompression uncompressed;

timeFormat      general;

timePrecision   6;

runTimeModifiable yes;

adjustTimeStep  yes;

maxCo           0.5;

maxDeltaT       1;


